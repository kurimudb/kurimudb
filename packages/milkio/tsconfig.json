{
  "compilerOptions": {
    "composite": true,
    "target": "esnext",
    "jsx": "react-jsx",
    "lib": [
      "ESNext"
    ],
    "moduleDetection": "force",
    "rootDir": "./../../../",
    "module": "esnext",
    "moduleResolution": "bundler",
    "paths": {
      "../../*": [
        "../../*"
      ]
    },
    "types": [
      "bun-types"
    ],
    "allowImportingTsExtensions": true,
    "allowJs": true,
    "strict": true,
    "downlevelIteration": true,
    "noEmit": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  }
}
